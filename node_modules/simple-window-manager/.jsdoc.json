{
    "applicationName": "simple-window-manager",
    "meta": {
        "title": "simple-window-manager",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./src",
            "./node_modules/eventemitter3"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "readme": "./README.md",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/jsdoc",
        "template": "../jsdoc-template"
    },
    "plugins": [
        "./node_modules/yy-jsdoc-template/plugins/skip-node_modules.js"
    ],
    "sourceType": "module",
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "simple-window-manager",
        "footer"        : "by David Figatner (david@yopeyopey.com)",
        "copyright"     : "Copyright Â© 2020 YOPEY YOPEY LLC.",
        "meta": {
            "title": "simple-window-manager API Documentation",
            "description": "Documentation for simple-window-manager library",
            "keyword": "docs, documentation, html5, javascript, jsdoc, window manager"
        }
    }
}